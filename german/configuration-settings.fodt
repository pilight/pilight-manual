      <text:p text:style-name="P307"><text:span text:style-name="T29">&quot;</text:span><text:span text:style-name="T32">smtp-host</text:span><text:span text:style-name="T29">&quot;</text:span><text:span text:style-name="T75">: </text:span><text:span text:style-name="T29">&quot;</text:span><text:span text:style-name="T32">smtp.foo.com</text:span><text:span text:style-name="T29">&quot;</text:span></text:p>
