   <text:list xml:id="list232112187558182" text:continue-list="list232110678100783" text:style-name="L4">
    <text:list-header>
     <text:p text:style-name="P80">Die Antwort auf ein get Kommando sieht so aus:</text:p>
    </text:list-header>
