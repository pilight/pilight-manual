    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P67"><text:tab/>&quot;type&quot;: &quot;set&quot;,</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P67"><text:tab/>&quot;key&quot;: &quot;pilight.version&quot;,</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P67"><text:tab/>&quot;value&quot;: &quot;6.0&quot;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P67">}</text:p>
    </text:list-item>
   </text:list>
   <text:p text:style-name="P24"/>
   <text:list xml:id="list232112463598349" text:continue-list="list232111374472213" text:style-name="Code_20_Numbering">
