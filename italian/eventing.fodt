     <text:p text:style-name="P123"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc354_1420391157" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">AND e OR<text:tab/>2</text:a></text:p>
     <text:p text:style-name="P123"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc356_1420391157" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Parentesi<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc945_614593705" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Operatori<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc947_614593705" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Dispositivi<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc949_614593705" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Funzioni<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc3037_673823619" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Azioni<text:tab/>4</text:a></text:p>
