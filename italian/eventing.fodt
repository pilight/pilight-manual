     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc1342_1085664404" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Introduzione<text:tab/>1</text:a></text:p>
     <text:p text:style-name="P122"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc3033_673823619" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Analisi delle regole<text:tab/>1</text:a></text:p>
