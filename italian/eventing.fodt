     <text:p text:style-name="P93"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc356_1420391157" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Parentesi<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P92"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc945_614593705" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Operatori<text:tab/>3</text:a></text:p>
     <text:p text:style-name="P92"><text:a xlink:type="simple" xlink:href="#__RefHeading___Toc947_614593705" text:style-name="Index_20_Link" text:visited-style-name="Index_20_Link">Dispositivi<text:tab/>3</text:a></text:p>
