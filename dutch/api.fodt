     <text:p text:style-name="P62"><text:tab/>&quot;action&quot;: &quot;registry&quot;,</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P62"><text:tab/>&quot;type&quot;: &quot;set&quot;,</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P62"><text:tab/>&quot;key&quot;: &quot;pilight.version&quot;,</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P62"><text:tab/>&quot;value&quot;: &quot;6.0&quot;</text:p>
    </text:list-item>
    <text:list-item>
     <text:p text:style-name="P62">}</text:p>
    </text:list-item>
   </text:list>
   <text:p text:style-name="P22"/>
